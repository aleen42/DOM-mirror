{
  "manifest_version": 2,
  "name": "DOM Scraper",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "DOM Scraper"
  },
  "permissions": [
    "activeTab",
    "downloads"
  ]
}
